<p id="notice"><%= notice %></p>

<h1>Denounces</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @denounces.each do |denounce| %>
      <tr>
        <td><%= denounce.content %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Denounce', new_denounce_path %>
